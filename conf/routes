GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /                           controllers.HomeController.index

GET     /usersReinnit                controllers.RegistrationController.reInnit

GET     /register                   controllers.RegistrationController.showRegistration(err: String)
POST    /register                   controllers.RegistrationController.submitRegistration
GET     /addUser                    controllers.RegistrationController.addUser(username:String, email :String, password :String)

GET     /logout                     controllers.LoginController.logout
GET     /login                      controllers.LoginController.showLogin(err: String)
POST    /login                      controllers.LoginController.submitLogin()